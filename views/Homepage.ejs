<body>


    <!-- HEADER -->
    <%- include('navbar', {page: ""}) %>


   <!-- GRID -->
   <div class="container">
      <div class="row">
        <div>
          <h2>one-way ticket</h2>
        </div>
      
          <div class="col-12 offset-lg-6 col-lg-6">
            <!--New City section-->
            <form action="/trajet" method="POST">
              <div class="input-group mb-3 ">
                <input class="form-control p-2 mx-1" name="departure" type="text" class="form-control" placeholder="enter depart city " aria-label="new city name">
                <input class="form-control p-2 mx-1" name="arrival" type="text" class="form-control" placeholder="enter arrived city" aria-label="new city name">
                <input class="form-control p-2 mx-1" name="date" type="date" name="calendrier">
                <div class="input-group-append mx-1">
                  <button class="btn btn-warning" type="submit" id="button-addon2">GO</button>
                </div>
              </div>
            </form>
            <div>
              <img src="./images/landmark.png" alt="">
            </div>
          </div>
          
      </div>
   </div>
   

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
